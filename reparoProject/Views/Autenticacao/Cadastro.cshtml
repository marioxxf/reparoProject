@{
    ViewBag.Title = "Autenticacao";
    Layout = "~/Views/Shared/_Layout.cshtml";
    string idDaSessao = Session.SessionID;
}
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>

<style>
    .container {
        width: 50vw;
        height: 65vh;
        display: flex;
        flex-direction: row;
        justify-content: center;
        align-items: center;
    }

    .centraliza {
        display: flex;
        flex-direction: row;
        justify-content: center;
        align-items: center;
    }
</style>

<div id="divCadastro">
    <div class="container">
        <div class="jumbotron">
            <div class="centraliza">
                <h2 style="color: white; font-size: 20px;">Cadastro no Repariano&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</h2>
                <div class="d-flex" style="height: 200px;">
                    <div class="vr" style="margin-right: 20px;"></div>
                </div>
                <div>
                    <div class="form-group" style="margin-right: 20px;">
                        <a href="/autenticacao/cadastro/cliente" class="btn btn-secondary" style="background: white; color: rgba(0,0,0,.5); border: white; ">Sou um cliente</a><br /><br />
                    </div>
                    <div class="form-group">
                        <a href="/autenticacao/cadastro/luthier" class="btn btn-secondary" style="background: white; color: rgba(0,0,0,.5); border: white; ">Sou um luthier</a><br /><br />
                    </div>
                </div>
            </div>
            
        </div>
    </div>
</div>

<script>
    $().ready(function () {
        document.getElementById('idSessao').value = '@idDaSessao';
    });
</script>